{
    "name": "wyrihaximus/react-stream-json",
    "description": "Incremental JSON encoder writing to a react/stream",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^8",
        "ext-json": "*",
        "api-clients/rx": "^2.2",
        "evenement/evenement": "^3.0",
        "react/promise": "^2.7",
        "react/stream": "^1.1.0",
        "reactivex/rxphp": "^2.0",
        "thecodingmachine/safe": "^1.3"
    },
    "require-dev": {
        "react/promise-stream": "^1.2",
        "wyrihaximus/async-test-utilities": "^4.2 || ^5.0",
        "wyrihaximus/ticking-promise": "^3"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\Stream\\Json\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\React\\Tests\\Stream\\Json\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "infection/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "icanhazstring/composer-unused": true,
            "ergebnis/composer-normalize": true
        },
        "platform": {
            "php": "8.0.12"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
